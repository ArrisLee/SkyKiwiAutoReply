{
  "name": "SkyKiwiAutoReply",
  "manifest_version": 2,
  "version": "2.0.0",
  "icons": { "16": "logo.png","48": "logo.png","128": "logo.png" },
  "description": "A tiny app which could be utilized to auto reply threads on Skykiwi BBS",
  "content_scripts": [
    {
      "matches": ["http://bbs.skykiwi.com/forum.php?mod=viewthread&tid=<your_thread_id_here>*"],
      "js": ["myscript.js"]
    }
  ]
}
